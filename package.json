{
  "name": "vite-project-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:esm": "vue-tsc && cross-env ESM=true vite build",
    "preview": "vite preview",
    "prettier": "tsc --noEmit && prettier src/**/*.{vue,js,ts}{,x} --no-error-on-unmatched-pattern --check",
		"lint": "tsc --noEmit && eslint src/**/*.{vue,js,ts}{,x} --no-error-on-unmatched-pattern --ignore-pattern node_modules/",
		"lint:fix": "npm run lint -- --fix",
		"prettier:fix": "npm run prettier -- --write",
		"format": "npm run prettier:fix && npm run lint:fix",
		"prepare": "git config core.autocrlf false && npx husky install",
		"pre-commit": "lint-staged"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.4.2",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "commitlint": "^17.4.2",
    "cross-env": "^7.0.3",
    "cz-git": "^1.4.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "quicktype-core": "^21.0.2",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.0",
    "vue-tsc": "^1.0.11"
  },
  "eslintConfig": {
    "extends": [
      "./config/eslint.config.cjs"
    ]
  },
  "prettier": "./config/prettier.config.cjs",
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "npm run prettier && npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "style",
          "test",
          "fix",
          "docs",
          "release",
          "update",
          "perf",
          "chore"
        ]
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-git",
      "czConfig": "./config/cz.config.cjs"
    }
  }
}
